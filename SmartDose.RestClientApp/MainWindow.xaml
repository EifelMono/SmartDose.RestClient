<Window
    x:Class="SmartDose.RestClientApp.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:forms="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
    xmlns:local="clr-namespace:SmartDose.RestClientApp"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:views="clr-namespace:SmartDose.RestClientApp.Views"
    Title="MainWindow"
    Width="800"
    Height="450"
    Loaded="Window_Loaded"
    mc:Ignorable="d">

    <StackPanel>
        <Button
            Name="MedicineCreate"
            Click="MedicineCreate_Click"
            Content="Medicine Create" />
        <Button
            Name="MedicineGet"
            Click="MedicineGet_Click"
            Content="Medicine GetList" />
        <Button
            Name="MedicineGetList"
            Click="MedicineGetList_Click"
            Content="Medicine Get" />


    </StackPanel>
</Window>
